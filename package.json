{
  "name": "boomerang",
  "version": "1.0.0",
  "description": "Boomerang - is clean, modern and responsive Bootstrap 4 template developed for corporate or any other business company sites to showcase their company, services, portfolio, team, blogs in most professional looking manner. The Boomerang template supports parallax, Vimeo, Youtube and local video backgrounds. Template also includes blog pages, portfolio pages and shop pages.",
  "main": "index.js",
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "nextBookingTime": "node nextBookingTime.js",
    "preinst": "npx npm-force-resolutions",
    "start": "npm run serve",
    "build": "npm run compile:pug && npm run compile:sass && npm run copy:jquery && npm run copy:plugins && npm run copy:bootstrap && npm run copy:js-core && npm run copy:mainJS && npm run copy:css && npm run compile:js && npm run copy:php && npm run copy:images && npm run copy:fonts && npm run copy:video",
    "compile:pug": "gulp compile:pug",
    "compile:sass": "gulp compile:sass",
    "copy:jquery": "gulp copy:jquery",
    "copy:plugins": "gulp copy:plugins",
    "copy:bootstrap": "gulp copy:bootstrap",
    "copy:js-core": "gulp copy:js-core",
    "copy:mainJS": "gulp copy:mainJS",
    "copy:css": "gulp copy:css",
    "watch": "gulp watch",
    "compile:js": "babel app/assets/js/custom/custom.js --out-file dist/assets/js/custom/custom.js && npm run minify:js && npm run minify:mainMin",
    "copy:php": "gulp copy:php",
    "copy:images": "gulp copy:images",
    "copy:fonts": "gulp copy:fonts",
    "copy:video": "gulp copy:video",
    "minify:css": "uglifycss dist/assets/css/template.css > dist/assets/css/template.min.css",
    "minify:js": "uglifyjs dist/assets/js/custom/custom.js -o dist/assets/js/custom/custom.min.js",
    "minify:mainMin": "uglifyjs dist/assets/js/custom/main.js -o dist/assets/js/custom/main.min.js",
    "serve": "node index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "babel-cli": "^6.26.0",
    "body-parser": "^1.20.3",
    "boomerang": "file:",
    "boomerang---1.3": "file:",
    "browserslist": "^4.23.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "graceful-fs": "^4.2.11",
    "gulp-cli": "^2.3.0",
    "mongoose": "^8.2.0",
    "nodemailer": "^6.9.15"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "browser-sync": "^3.0.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "perfectionist": "^2.4.0",
    "pug": "^3.0.3",
    "rimraf": "^6.0.1",
    "sass": "^1.79.4",
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  },
  "resolutions": {
    "graceful-fs": "^4.2.11"
  }
}
